{
  "name": "dog",
  "version": "0.4.1",
  "description": "Developer blOG, markdown based, made to be simple and fast, yet feature rich.",
  "main": "index.js",
  "bin": "index.js",
  "engines": {
    "node": ">=0.6"
  },
  "scripts": {
    "test": "tap ./test/*.js",
    "example-site": "cd examples/blog-site/site; node app",
    "example-preview": "node . preview -r ./examples/blog-only -p first-post; node . preview -r ./examples/blog-only -p post-with-rendered-metadata"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/dog.git"
  },
  "keywords": [
    "markdown",
    "blog",
    "highlight",
    "code",
    "snippets"
  ],
  "author": "Thorsten Lorenz <thlorenz@gmx.de> (http://thlorenz.com)",
  "license": "BSD",
  "dependencies": {
    "ap": "~0.1.0",
    "dateformat": "~1.0.0",
    "marked": "~0.2.10",
    "node-syntaxhighlighter": "~0.8.1",
    "npmlog": "0.0.2",
    "optimist": "~0.3.7",
    "peacock": "~0.2.0",
    "runnel": "0.0.5"
  },
  "devDependencies": {
    "proxyquire": "~0.4.1",
    "tap": "~0.4.13"
  }
}
